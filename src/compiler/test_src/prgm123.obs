use Introspection;

bundle Default {
	class Foo {
		@a : Int;
		@b : Int;

		New() {
			@a := 13;
			@b := 7;
		}

		method : public : A() ~ Int {
			for(i := 0; i < 10000; i += 1;) {
				@a += @b;
			};

			return (@a * @b) / 2;
		}
	}
  
	class Hello {
		function : Main(args : String[]) ~ Nil {
			Run();
		}

		function : Run() ~ Nil {
			count := 0;
			for(i := 0; i < 100; i += 1;) {
				a := Foo->New();
				count += a->A();				
			};
			count->PrintLine();
		}

		function : Boo(v : String) ~ Base {
			return "hendrix";
		}
	}
}
